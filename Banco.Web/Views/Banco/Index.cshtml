@model IEnumerable<Banco.Entidades.BancoBe>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
} 
 <div class="panel-body">
    <div>
        <a href="/Banco/Create">Registrar</a>
    </div>
    @(Html.TableFor<Banco.Entidades.BancoBe>()
        .Columns(column =>
        {
            column.Expression(p => p.IdBanco).Title("ID");
            column.Expression(p => p.Nombre).Title("Nombre");
            column.Expression(p => p.Direccion).Title("Direccion");
            column.Expression(p => p.FechaRegistroStr).Title("Fecha Registro");

        })
        .DataSource(this.Model)
        .ToHtml("basicTable", "table-bordered table-responsive", "/Banco/Edit?id=", "/Banco/Delete?id=")
    )
</div>

 

